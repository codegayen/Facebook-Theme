{
   "background": {
      "page": "pages/background.html"
   },
   "browser_action": {
      "default_icon": "images/128.png",
      "default_popup": "pages/popup.html",
      "default_title": "__MSG_app_name__"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "styles/inject.css" ],
      "matches": [ "*://*.facebook.com/*" ],
      "run_at": "document_start"
   } ],   
   "default_locale": "en_US",
   "description": "__MSG_app_description__",
   "homepage_url": "https://www.codehemu.com/2022/04/facebook-theme.html",
   "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "64": "images/64.png"
   },
   "manifest_version": 2,
   "name": "__MSG_app_name__",
   "permissions": [ "storage", "webRequest", "http://*/*", "https://*/*" ],
   "short_name": "FacebookTheme",
   "version": "3.3.3"
}
